{
	"name": "components",
	"version": "0.6.0-alpha",
	"description": "Components for Obsidian",
	"main": "main.js",
	"scripts": {
		"predev": "node scripts/register.mjs",
		"dev": "node esbuild.config.mjs",
		"prebuild": "node scripts/register.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:extended": "cross-env BUILD_TIER=extended npm run build",
		"dev:extended": "cross-env BUILD_TIER=extended npm run dev",
		"test": "npx tsx --tsconfig tests/tsconfig.json tests/expression.test.ts && npx tsx --tsconfig tests/tsconfig.json tests/utils.test.ts",
		"vbump": "node version-bump.mjs"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"cross-env": "^7.0.3",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"tsx": "^4.20.6",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"muuri": "^0.9.5"
	}
}
